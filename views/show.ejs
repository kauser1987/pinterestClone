<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-800">
    <div class="cards flex flex-wrap px-4 gap-10">
        <% user.posts.forEach((elem)=>{ %>
            <div class="text-white w-32">
            <div class="w-full h-[10vw] bg-sky-200 rounded-lg overflow-hidden">
                <img class="w-full h-full object-cover " src="/images/uploads/<%= elem.postImage %>" alt="">
            </div>
            <h5 class="text-xs font-semibold mt-1"><%= elem.title %></h5>
            <h5 class="text-xs font-semibold"><%=elem.user.username%></h5>
        </div>
        <% })%>
    </div>

</div>

<% include ./partials/footer.ejs %>

